   "cell_type": "code",
   "execution_count": 88,
   "id": "35d083a3-2d79-45f1-ac08-441d3a43daf5",
   "metadata": {
    "tags": []
   },
   "outputs": [],
   "source": [
    "def expand_mean(df):\n",
    "    return df.expanding(1).mean() \n",
    "\n",
    "for col in [\"tmax\", \"tmin\", \"prcp\"]:\n",
    "    weather[f\"month_avg{col}\"] = weather[col].groupby(weather.index.month, group_keys=False).apply(expand_mean)\n",
    "    weather[f\"day_avg{col}\"] = weather[col].groupby(weather.index.day_of_year, group_keys=False).apply(expand_mean) \n",
    "    "
   ]
  },
